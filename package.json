{
  "name": "react-weather-app",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://bukovski.github.io/react-weather-app/",
  "keywords": [
    "react",
    "react api",
    "react sass",
    "react weather",
    "react forecast",
    "react diagram",
    "react ipinfo",
    "react here.com",
    "react autocomplete"
  ],
  "author": "Kirill Bukovski",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "animated-number-react": "^0.1.2",
    "apexcharts": "^3.28.1",
    "gh-pages": "^3.2.3",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "vercel-toast": "^1.5.3",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "coveralls": "^3.1.1",
    "jest-fetch-mock": "^3.0.3",
    "node-sass": "^6.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:snapshot": "npm test -m",
    "test:coverage": "CI=true npm test -- --env=jsdom --coverage",
    "test:coveralls": "CI=true npm test -- --env=jsdom --coverage && coveralls < coverage/lcov.info",
    "eject": "react-scripts eject",
    "scss": "node-sass src/resources/sass/main.sass -o src/",
    "watch-scss": "node-sass --watch src/resources/sass/main.sass -o src/",
    "build-scss": "npm run scss",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/libs/**/*.{js,jsx}",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/reportWebVitals.js",
      "!<rootDir>/src/components/diagram.component.jsx",
      "!<rootDir>/src/hock-context/themeContext.js"
    ],
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ]
  }
}
