{
  "name": "react-weather-app",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://github.com/Bukovski/react-weather-app",
  "keywords": [
    "sass",
    "react",
    "api"
  ],
  "author": "Kirill Bukovski",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "node-sass": "^6.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "scss": "node-sass src/resources/sass/main.sass -o src/",
    "watch-scss": "node-sass --watch src/resources/sass/main.sass -o src/",
    "build-scss": "npm run scss"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
